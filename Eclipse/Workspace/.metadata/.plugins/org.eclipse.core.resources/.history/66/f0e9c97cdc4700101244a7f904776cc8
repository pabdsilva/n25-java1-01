package br.com.senaisp.bauru.classes;

public class Estudante {
	// Fields ou campos ou variáveis de instância
	private String nome;
	private int ra;
	private double nota;
	private boolean ehFumante;

	// Constructor
	public Estudante() {
		setNome("Sem nome");
		ran = 0;
		nota = 0,0;
		eh fumante = false;
	}
}
